{
    "id": "$root",
    "type": "object",
    "properties": {
        "code": {
            "id": "$root/properties/code",
            "type": "integer",
            "title": "title of code",
            "description": "desc of code",
            "default": 0
        },
        "msg": {
            "id": "$root/properties/msg",
            "type": "string",
            "description": "desc of msg",
            "title": "title of msg",
            "default": ""
        },
        "isOk": {
            "id": "$root/properties/isOk",
            "type": "boolean",
            "title": "title of isOk",
            "description": "desc of isOk",
            "default": true,
            "enum": [
                true,
                false
            ],
            "$uiEnum": [
                {
                    "label": "是",
                    "value": true
                },
                {
                    "label": "否",
                    "value": false
                }
            ]
        },
