                            "status": {
                                "id": "$root/properties/data/properties/list/items/properties/status",
                                "type": "integer",
                                "title": "状态",
                                "description": "desc of status",
                                "default": 0,
                                "$uiControl": "Select"
                            },
                            "VideoPlayUrl": {
                                "id": "$root/properties/data/properties/list/items/properties/VideoPlayUrl",
                                "type": "string",
                                "description": "desc of mediaFiles.VideoPlayUrl",
                                "title": "视频播放地址",
                                "default": "32"
                            }
                        }
                    }
