                                "default": 123
                            },
                            "title": {
                                "id": "$root/properties/data/properties/list/items/properties/title",
                                "type": "string",
                                "description": "desc of title",
                                "title": "视频标题",
                                "default": "apple"
                            },
                            "musicUrl": {
                                "id": "$root/properties/data/properties/list/items/properties/musicUrl",
                                "type": "string",
                                "description": "desc of musicUrl",
                                "title": "背景音乐",
                                "default": "ewe"
                            },
                            "challengeName": {
                                "id": "$root/properties/data/properties/list/items/properties/challengeName",
                                "type": "string",
                                "description": "desc of challengement",
                                "title": "挑战",
                                "default": "ewqe"
                            },
                            "uid": {
                                "id": "$root/properties/data/properties/list/items/properties/uid",
                                "type": "string",
                                "description": "desc of uid",
                                "title": "发布人ID",
                                "default": "2011"
                            },
                            "nickname": {
                                "id": "$root/properties/data/properties/list/items/properties/nickname",
                                "type": "string",
                                "description": "desc of nickname",
                                "title": "发布人昵称",
                                "default": "de"
                            },
                            "publishTime": {
                                "id": "$root/properties/data/properties/list/items/properties/publishTime",
                                "type": "string",
                                "description": "发布时间",
                                "title": "title of publishTime",
                                "default": "432",
                                "$uiControl": "DatePicker"
                            },
                            "source": {
                                "id": "$root/properties/data/properties/list/items/properties/source",
                                "type": "string",
                                "description": "desc of source",
                                "title": "类别",
                                "default": "leibie"
                            },
                            "cpName": {
                                "id": "$root/properties/data/properties/list/items/properties/cpName",
                                "type": "string",
                                "description": "desc of cpName",
                                "title": "视频来源",
                                "default": "laiyuan"
                            },
                            "insertTime": {
                                "id": "$root/properties/data/properties/list/items/properties/insertTime",
                                "type": "string",
                                "description": "desc of insertTime",
                                "title": "添加时间",
                                "default": "324",
                                "$uiControl": "DatePicker"
                            },
                            "playCount": {
                                "id": "$root/properties/data/properties/list/items/properties/playCount",
                                "type": "integer",
                                "title": "播放次数",
                                "description": "desc of playCount",
                                "default": 0,
                                "$uiIsSort": true
                            },
                            "likeCount": {
                                "id": "$root/properties/data/properties/list/items/properties/likeCount",
                                "type": "integer",
                                "title": "获赞数",
                                "description": "desc of likeCount",
                                "default": 0,
                                "$uiIsSort": true
                            },
